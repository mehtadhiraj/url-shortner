{
  "name": "short-url-service",
  "version": "1.0.0",
  "description": "Short URL Service",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app/app.js",
    "build": "./node_modules/typescript/bin/tsc",
    "dev": "./node_modules/typescript/bin/tsc & nodemon app/app.ts",
    "create-migration": "knex --knexfile app/data/db/knexfile.ts migrate:make",
    "migrate": "knex --knexfile app/data/db/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile app/data/db/knexfile.ts migrate:rollback"
  },
  "module": "commonjs",
  "author": "Dhiraj Mehta",
  "license": "ISC",
  "dependencies": {
    "@tsed/core": "7.18.2",
    "@tsed/di": "7.18.2",
    "@tsed/logger": "6.3.4",
    "@tsed/objection": "7.18.2",
    "@tsed/schema": "7.18.2",
    "@types/knex": "0.16.1",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "axios": "1.3.4",
    "body-parser": "1.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "5.0.0",
    "convict": "6.2.4",
    "express": "4.18.1",
    "express-json-validator-middleware": "3.0.1",
    "ioredis": "5.3.1",
    "kafkajs": "^2.2.4",
    "knex": "2.4.0",
    "module-alias": "2.2.2",
    "nodemon": "2.0.20",
    "objection": "3.0.1",
    "openapi3-ts": "2.0.2",
    "pg": "8.9.0",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.10.2",
    "routing-controllers-openapi": "4.0.0",
    "supertest": "6.3.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.2",
    "ts-convict": "1.1.0",
    "ts-node": "10.8.1",
    "tsc": "2.0.4",
    "tslib": "2.4.0",
    "typedi": "0.10.0",
    "uuid": "9.0.0",
    "winston": "^3.17.0"
